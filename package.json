{
  "name": "pdf-export",
  "version": "1.1.0",
  "description": "Export Obsidian notes to PDF with image support for iOS",
  "main": "build/main.js",
  "type": "module",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [
    "obsidian",
    "pdf",
    "export",
    "multi-page"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.25.11",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "obsidian": "^1.10.2-1"
  }
}
